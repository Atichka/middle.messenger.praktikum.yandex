{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es2015",
    "module": "es2015",
    "declaration": false,
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "lib": ["dom"],
}
